# OCR服务配置
OCR_PORT=8001
OCR_HOST=127.0.0.1
OCR_PATH=/ocr
OCR_URL=http://${OCR_HOST}:${OCR_PORT}${OCR_PATH}

# LLM服务配置
LLM_MODEL_NAME=mimo-v2-flash
LLM_URL=https://api.xiaomimimo.com/v1
LLM_API_KEY=your_llm_api_key_here

# VLM服务配置
VLM_MODEL_NAME=qwen3-vl-32b-instruct
VLM_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
VLM_API_KEY=your_vlm_api_key_here

# 嵌入式服务配置
EMBEDDING_PORT=8002
EMBEDDING_HOST=127.0.0.1
EMBEDDING_MODEL_NAME=Qwen/Qwen3-Embedding-0.6B

# 重排序服务配置
RERANK_PORT=8003
RERANK_HOST=127.0.0.1
RERANK_MODEL_NAME=BAAI/bge-reranker-v2-m3

# 搜索服务配置
SEARCH_SERVICE_PORT=8004
SEARCH_SERVICE_HOST=127.0.0.1

# SearXNG 搜索引擎配置
SEARXNG_PORT=8005
SEARXNG_URL=http://searxng:8080
SEARXNG_TIMEOUT=5
SEARXNG_LIMITER=false
SEARXNG_ENGINES=bing,baidu
SEARXNG_LANGUAGE=zh-CN

# PostgreSQL数据库配置
PGSQL_PORT=5432
PGSQL_HOST=127.0.0.1
PGSQL_USER=postgres
PGSQL_PASSWORD=your_password_here
PGSQL_DATABASE=myagent

# Chat 服务配置
CHAT_SERVICE_PORT=8006
CHAT_SERVICE_HOST=0.0.0.0
CHAT_SERVICE_DEBUG=true

# Chat LLM配置（独立配置，优先于全局LLM配置）
CHAT_LLM_MODEL_NAME=mimo-v2-flash
CHAT_LLM_URL=https://api.xiaomimimo.com/v1
CHAT_LLM_API_KEY=your_chat_llm_api_key_here

# Chat 参数配置
CHAT_DEFAULT_TEMPERATURE=0.7
CHAT_DEFAULT_MAX_TOKENS=2048
CHAT_DEFAULT_TOP_P=0.9
CHAT_MAX_CONTEXT_MESSAGES=100
CHAT_MAX_CONTEXT_TOKENS=8000

# Web 前端配置
FRONTEND_PORT=5173
FRONTEND_HOST=0.0.0.0
FRONTEND_URL=http://localhost:5173

# Vite 前端 API 配置
VITE_API_URL=http://localhost:8000/api

# DeepSearch 服务配置
DEEPSEARCH_SERVICE_PORT=8007
DEEPSEARCH_SERVICE_HOST=127.0.0.1
DEEPSEARCH_MAX_ITERATIONS=3
DEEPSEARCH_QUERIES_PER_ITERATION=3

# Milvus 向量数据库配置
MILVUS_HOST=127.0.0.1
MILVUS_PORT=19531
MILVUS_USER=
MILVUS_PASSWORD=

# Sandbox 沙盒服务配置
SANDBOX_SERVICE_PORT=8010
SANDBOX_SERVICE_HOST=127.0.0.1
SANDBOX_EXECUTION_TIMEOUT=120
SANDBOX_MEMORY_LIMIT=256m
SANDBOX_CPU_LIMIT=1.0

# Agent 智能体服务配置
AGENT_SERVICE_PORT=8009
AGENT_SERVICE_HOST=0.0.0.0
AGENT_SERVICE_DEBUG=true

# Agent LLM配置（独立配置，可复用全局 LLM）
AGENT_LLM_MODEL_NAME=mimo-v2-flash
AGENT_LLM_URL=https://api.xiaomimimo.com/v1
AGENT_LLM_API_KEY=your_agent_llm_api_key_here

# Agent 参数配置
AGENT_MAX_ITERATIONS=10
AGENT_MAX_CONTEXT_TOKENS=16000
AGENT_DEFAULT_TEMPERATURE=0.7
AGENT_DEFAULT_MAX_TOKENS=4096
AGENT_KNOWLEDGE_SPACE_PATH=/app/storage/knowledge

# Agent 并发控制与超时配置
AGENT_MAX_CONCURRENT_REQUESTS=10    # 最大并发请求数（建议5-20）
AGENT_REQUEST_TIMEOUT=1200           # 整体请求超时时间（秒，默认5分钟）
AGENT_LLM_CALL_TIMEOUT=120           # 单次LLM调用超时（秒）
AGENT_TOOL_TIMEOUT=120               # 单次工具执行超时（秒）

# RAG 服务配置
RAG_SERVICE_PORT=8008
RAG_SERVICE_HOST=127.0.0.1
RAG_COLLECTION_NAME=documents
RAG_TOP_K=10
RAG_SIMILARITY_THRESHOLD=0.7

HF_TOKEN=your_huggingface_token_here
