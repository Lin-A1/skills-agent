# =============================================================================
# MyAgent 环境配置示例
# MyAgent Environment Configuration Example
# =============================================================================
# 复制此文件为 .env 并填写实际值
# Copy this file to .env and fill in actual values

# -----------------------------------------------------------------------------
# OCR 服务配置 | OCR Service Configuration
# -----------------------------------------------------------------------------
OCR_PORT=8001
OCR_HOST=ocr_service
OCR_PATH=/ocr
OCR_URL=http://${OCR_HOST}:${OCR_PORT}${OCR_PATH}

# -----------------------------------------------------------------------------
# LLM 服务配置 | LLM Service Configuration (Global)
# -----------------------------------------------------------------------------
LLM_MODEL_NAME=your-model-name
LLM_URL=https://api.your-llm-provider.com/v1
LLM_API_KEY=sk-your-api-key-here

# -----------------------------------------------------------------------------
# VLM 视觉语言模型配置 | Vision Language Model Configuration
# -----------------------------------------------------------------------------
VLM_MODEL_NAME=your-vlm-model
VLM_URL=https://api.your-vlm-provider.com/v1
VLM_API_KEY=sk-your-vlm-api-key-here

# -----------------------------------------------------------------------------
# 嵌入服务配置 | Embedding Service Configuration
# -----------------------------------------------------------------------------
EMBEDDING_PORT=8002
EMBEDDING_HOST=embedding_service
EMBEDDING_MODEL_NAME=Qwen/Qwen3-Embedding-0.6B

# -----------------------------------------------------------------------------
# 重排序服务配置 | Rerank Service Configuration
# -----------------------------------------------------------------------------
RERANK_PORT=8003
RERANK_HOST=rerank_service
RERANK_MODEL_NAME=BAAI/bge-reranker-v2-m3

# -----------------------------------------------------------------------------
# 联网搜索服务配置 | Web Search Service Configuration
# -----------------------------------------------------------------------------
SEARCH_SERVICE_PORT=8004
SEARCH_SERVICE_HOST=websearch_service

# -----------------------------------------------------------------------------
# SearXNG 搜索引擎配置 | SearXNG Search Engine Configuration
# -----------------------------------------------------------------------------
SEARXNG_PORT=8005
SEARXNG_URL=http://searxng:8080
SEARXNG_TIMEOUT=5
SEARXNG_LIMITER=false
SEARXNG_ENGINES=bing,baidu
SEARXNG_LANGUAGE=zh-CN

# -----------------------------------------------------------------------------
# PostgreSQL 数据库配置 | PostgreSQL Database Configuration
# -----------------------------------------------------------------------------
PGSQL_PORT=5432
PGSQL_HOST=pgsql
PGSQL_USER=postgres
PGSQL_PASSWORD=your-database-password
PGSQL_DATABASE=myagent

# -----------------------------------------------------------------------------
# Chat 服务配置 | Chat Service Configuration
# -----------------------------------------------------------------------------
CHAT_SERVICE_PORT=8006
CHAT_SERVICE_HOST=0.0.0.0
CHAT_SERVICE_DEBUG=true

# Chat LLM 配置（独立配置，优先于全局 LLM 配置）
# Chat LLM Configuration (Independent, takes precedence over global LLM config)
CHAT_LLM_MODEL_NAME=your-chat-model
CHAT_LLM_URL=https://api.your-llm-provider.com/v1
CHAT_LLM_API_KEY=sk-your-chat-api-key-here

# Chat 参数配置 | Chat Parameters
CHAT_DEFAULT_TEMPERATURE=0.7
CHAT_DEFAULT_MAX_TOKENS=2048
CHAT_DEFAULT_TOP_P=0.9
CHAT_MAX_CONTEXT_MESSAGES=100
CHAT_MAX_CONTEXT_TOKENS=8000

# -----------------------------------------------------------------------------
# Web 前端配置 | Web Frontend Configuration
# -----------------------------------------------------------------------------
FRONTEND_PORT=5173
FRONTEND_HOST=0.0.0.0
FRONTEND_URL=http://localhost:5173

# Vite 前端 API 配置 | Vite Frontend API Configuration
VITE_API_URL=http://localhost:8000/api

# -----------------------------------------------------------------------------
# DeepSearch 深度搜索服务配置 | DeepSearch Service Configuration
# -----------------------------------------------------------------------------
DEEPSEARCH_SERVICE_PORT=8007
DEEPSEARCH_SERVICE_HOST=deepsearch_service
DEEPSEARCH_MAX_ITERATIONS=3
DEEPSEARCH_QUERIES_PER_ITERATION=3

# -----------------------------------------------------------------------------
# Milvus 向量数据库配置 | Milvus Vector Database Configuration
# -----------------------------------------------------------------------------
MILVUS_HOST=milvus
MILVUS_PORT=19531
MILVUS_USER=
MILVUS_PASSWORD=

# -----------------------------------------------------------------------------
# Sandbox 沙盒服务配置 | Sandbox Service Configuration
# -----------------------------------------------------------------------------
SANDBOX_SERVICE_PORT=8010
SANDBOX_SERVICE_HOST=sandbox_service
SANDBOX_EXECUTION_TIMEOUT=120
SANDBOX_MEMORY_LIMIT=256m
SANDBOX_CPU_LIMIT=1.0

# -----------------------------------------------------------------------------
# RAG 服务配置 | RAG Service Configuration
# -----------------------------------------------------------------------------
RAG_SERVICE_PORT=8008
RAG_SERVICE_HOST=rag_service
RAG_COLLECTION_NAME=documents
RAG_TOP_K=10
RAG_SIMILARITY_THRESHOLD=0.7

# HuggingFace Token（用于模型下载）| HuggingFace Token (for model downloads)
HF_TOKEN=hf_your-huggingface-token

# -----------------------------------------------------------------------------
# Agent 服务配置 | Agent Service Configuration
# -----------------------------------------------------------------------------
AGENT_SERVICE_PORT=8009
AGENT_SERVICE_HOST=0.0.0.0
AGENT_SERVICE_DEBUG=true

# Agent LLM 配置（独立配置，优先于全局 LLM 配置）
# Agent LLM Configuration (Independent, takes precedence over global LLM config)
AGENT_LLM_MODEL_NAME=your-agent-model
AGENT_LLM_URL=https://api.your-llm-provider.com/v1
AGENT_LLM_API_KEY=sk-your-agent-api-key-here

# Agent 参数配置 | Agent Parameters
AGENT_DEFAULT_TEMPERATURE=0.3
AGENT_DEFAULT_MAX_TOKENS=4096
AGENT_DEFAULT_TOP_P=0.9
AGENT_MAX_ITERATIONS=10
AGENT_ITERATION_TIMEOUT=120

# Agent 上下文配置 | Agent Context Configuration
AGENT_MAX_CONTEXT_MESSAGES=50
AGENT_MAX_CONTEXT_TOKENS=16000

# -----------------------------------------------------------------------------
# Skills 配置 | Skills Configuration
# -----------------------------------------------------------------------------
# Skills 目录路径（Agent 自动发现 SKILL.md 文件）
# Skills directory path (Agent auto-discovers SKILL.md files)
SKILLS_DIRECTORY=/path/to/your/services
SKILLS_AUTO_DISCOVER=true
